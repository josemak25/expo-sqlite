{
  "name": "expo-queue",
  "version": "0.1.0",
  "description": "A robust queue system for Expo apps using SQLite",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./sqlite": {
      "source": "./src/adapters/sqlite.ts",
      "types": "./lib/typescript/src/adapters/sqlite.d.ts",
      "default": "./lib/module/adapters/sqlite.js"
    },
    "./async-storage": {
      "source": "./src/adapters/async-storage.ts",
      "types": "./lib/typescript/src/adapters/async-storage.d.ts",
      "default": "./lib/module/adapters/async-storage.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace expo-queue-example",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "test": "jest",
    "release": "release-it --only-version",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josemak25/expo-queue.git"
  },
  "author": "Amakiri Joseph <amakirij@gmail.com> (https://github.com/josemak25)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josemak25/expo-queue/issues"
  },
  "homepage": "https://github.com/josemak25/expo-queue#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.3.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.12",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "expo-sqlite": "^16.0.10",
    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "^0.40.17",
    "release-it": "^19.0.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "@react-native-community/netinfo": "*",
    "expo-sqlite": "*",
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": true
    },
    "@react-native-community/netinfo": {
      "optional": true
    },
    "expo-sqlite": {
      "optional": true
    }
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@4.11.0",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "create-react-native-library": {
    "type": "library",
    "languages": "js",
    "tools": [
      "jest",
      "release-it",
      "eslint"
    ],
    "version": "0.57.0"
  },
  "dependencies": {
    "@types/uuid": "^11.0.0",
    "eventemitter3": "^5.0.4",
    "uuid": "^13.0.0"
  }
}
